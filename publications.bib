@article{el-kebir_online_2021,
	series = {11th {IFAC} {Symposium} on {Biological} and {Medical} {Systems} {BMS} 2021},
	title = {Online {Hypermodel}-based {Path} {Planning} for {Feedback} {Control} of {Tissue} {Denaturation} in {Electrosurgical} {Cutting}},
	volume = {54},
