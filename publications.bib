	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.3109/02656736.2011.611962},
	keywords = {bioheat transfer, finite element modelling, laser-induced thermal therapy, MR temperature imaging, real-time control},
	pages = {751--761},
	file = {Full Text PDF:/Users/naveedriaziat/Zotero/storage/QYSEDWK4/Feng and Fuentes - 2011 - Model-based planning and real-time predictive cont.pdf:application/pdf},
}

@article{muddassir_model_2023,
	title = {Model {Predictive} {Thermal} {Dose} {Control} of a {Robotic} {Laser} {System} to {Automate} {Skin} {Photorejuvenation}},
	volume = {28},
	copyright = {https://ieeexplore.ieee.org/Xplorehelp/downloads/license-information/IEEE.html},
	issn = {1083-4435, 1941-014X},
	url = {https://ieeexplore.ieee.org/document/9955367/},
	doi = {10.1109/TMECH.2022.3218806},
	abstract = {In this paper, we present a new method to control the thermal stimulation of skin during a photorejuvenation procedure. The proposed method can precisely administer the thermal dose while controlling the tissue’s temperature under a safe limit. For that, a model-based treatment controller is developed and evaluated on a three-dimensional biophysicsbased numerical model of skin; A hardware implementation is experimentally tested on a gelatin-based phantom tissue subjected to pulsed laser irradiation. A key component of our method is the use of a new thermal dose metric that enables quantifying and controlling the skin photo-rejuvenation process; This metric represents a suitable alternative to the lack of consensus on the metrics used by the photodermatology community. The reported experiments demonstrate that the developed controller endowed with the proposed dose unit can automatically deliver a prescribed laser irradiation and thermal dose over the tissue surface. The signiﬁcance of our result is that it provides a control-theoretic framework to automate skin photorejuvenation treatments with thermal-guided robots. This approach has the potential to introduce standards in the automation of these types of photo-treatments.},
	language = {en},
	number = {2},
	urldate = {2024-10-15},
	journal = {IEEE/ASME Transactions on Mechatronics},
	author = {Muddassir, Muhammad and Limbert, Georges and Zhang, Bin and Duan, Anqing and Tan, Jun-Jong and Navarro-Alarcon, David},
	month = apr,
	year = {2023},
	pages = {737--747},
	file = {Muddassir et al. - 2023 - Model Predictive Thermal Dose Control of a Robotic.pdf:/Users/naveedriaziat/Zotero/storage/7HAGSVPV/Muddassir et al. - 2023 - Model Predictive Thermal Dose Control of a Robotic.pdf:application/pdf},
}

