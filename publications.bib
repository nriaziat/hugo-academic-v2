	keywords = {Thermal damage, Analytical solution, Moving heat source, Pennes equation, Tissue laser treatment},
	pages = {583--597},
	file = {Full Text PDF:/Users/naveedriaziat/Zotero/storage/DMH9HUGC/Kabiri and Talaee - 2021 - Thermal field and tissue damage analysis of moving.pdf:application/pdf},
}

