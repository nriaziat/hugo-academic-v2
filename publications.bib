	month = apr,
	year = {2020},
	note = {Conference Name: IEEE Transactions on Haptics},
	keywords = {Grippers, Task analysis, Manipulators, teleoperation, Grasping, Shared control, active constraints, grasping., Service robots, virtual fixtures},
	pages = {270--285},
	file = {IEEE Xplore Abstract Record:/Users/naveedriaziat/Zotero/storage/JZR4W3AC/stamp.html:text/html;IEEE Xplore Full Text PDF:/Users/naveedriaziat/Zotero/storage/9G6W5MKD/Abi-Farraj et al. - 2020 - A Haptic Shared-Control Architecture for Guided Mu.pdf:application/pdf},
