	keywords = {Computer Science - Robotics},
	file = {arXiv.org Snapshot:/Users/naveedriaziat/Zotero/storage/DA2IIIA9/2109.html:text/html;Fu et al. - 2022 - Targetless Extrinsic Calibration of Stereo Cameras.pdf:/Users/naveedriaziat/Zotero/storage/USFK5RRQ/Fu et al. - 2022 - Targetless Extrinsic Calibration of Stereo Cameras.pdf:application/pdf},
}

