@article{doumanidis_multivariable_2002,
	title = {Multivariable adaptive control of the bead profile geometry in gas metal arc welding with thermal scanning},
	volume = {79},
	issn = {0308-0161},
	url = {https://www.sciencedirect.com/science/article/pii/S0308016102000248},
