	keywords = {Computer Science - Computer Vision and Pattern Recognition, 92-10, 80-10, 93C40, 68T05, Electrical Engineering and Systems Science - Systems and Control, I.4.8, I.6.5},
	file = {arXiv.org Snapshot:/Users/naveedriaziat/Zotero/storage/YRMYN424/2203.html:text/html;Full Text PDF:/Users/naveedriaziat/Zotero/storage/FRZW6IWX/El-Kebir et al. - 2022 - Physics-based Learning of Parameterized Thermodyna.pdf:application/pdf},
